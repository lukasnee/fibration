{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "presentation": {
                "hidden": false,
                "group": "Fibration",
                "order": 1
            },
            "name": "Higgs",
            "device": "STM32F303RE",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/Build/Debug/Modules/Higgs/Higgs",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "configFiles": [
                // NOTE: interface, target order important
                "interface/stlink.cfg",
                "target/stm32f3x.cfg"
            ],
            "interface": "swd",
            "cpu": "cortex-m4",
            "preLaunchTask": "preLaunchTaskHiggs",
            "postDebugTask": "reset ST-LINK",
            "svdFile": "${workspaceRoot}/System/MSP/STM32F303.svd",
            "runToEntryPoint": "BlinkTestApp::Run",
            "preLaunchCommands": ["set remotetimeout 10", "show remotetimeout"],
            "openOCDLaunchCommands": [
                "program Build/Debug/Modules/Higgs/Higgs preverify verify reset",
                "$_TARGETNAME configure -rtos FreeRTOS -work-area-phys 0x20000000 -work-area-size 0x10000 -work-area-backup 0",
            ],
            // "rtos": "FreeRTOS",
            "showDevDebugOutput": false,
            "showDevDebugTimestamps": false,
            "numberOfProcessors": 1,
            "targetProcessor": 0,
        }
    ]
}